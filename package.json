{
  "name": "mau-mau-online",
  "private": true,
  "description": "\"Mau Mau\" é um jogo popular de cartas que é jogado com um baralho tradicional de 52 cartas. O objetivo do jogo é se livrar de todas as cartas na mão, jogando cartas que correspondem ao número ou ao naipe da carta na mesa.",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "build": "turbo run build",
    "changeset-create": "changeset",
    "changeset-apply": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mess-zone/mau-mau-online.git"
  },
  "author": "Gilmar Andrade",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mess-zone/mau-mau-online/issues"
  },
  "homepage": "https://github.com/mess-zone/mau-mau-online#readme",
  "devDependencies": {
    "turbo": "^1.9.3",
    "@changesets/cli": "^2.26.1"
  }
}
