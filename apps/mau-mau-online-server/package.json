{
  "name": "@mess-zone/mau-mau-online-server",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "description": "Servidor para jogar mau mau online",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "jest --passWithNoTests --watch --runInBand -c jest-integration.config.js",
    "build": "npm install && npx rimraf dist && npx tsc -p tsconfig-build.json && tsc-alias",
    "start": "node ./dist/main/server.js",
    "dev": "nodemon --exec ts-node ./src/server.ts"
  },
  "author": "Gilmar Andrade",
  "license": "MIT",
  "dependencies": {
    "module-alias": "^2.2.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
